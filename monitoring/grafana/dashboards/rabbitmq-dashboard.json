{
  "dashboard": {
    "title": "RabbitMQ Dashboard",
    "tags": ["rabbitmq", "messaging"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Messages Published",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rabbitmq_queue_messages_published_total[5m])",
            "legendFormat": "{{queue}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Messages Consumed",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rabbitmq_queue_messages_delivered_total[5m])",
            "legendFormat": "{{queue}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Queue Depth",
        "type": "graph",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages",
            "legendFormat": "{{queue}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Message Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(rabbitmq_queue_messages_acknowledged_total[5m])",
            "legendFormat": "{{queue}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Dead Letter Queue Messages",
        "type": "graph",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages{queue=~\"*.dlq\"}",
            "legendFormat": "{{queue}}"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}


